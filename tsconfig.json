{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react",
    "moduleResolution": "Bundler",
    "outDir": "jsxbundle",
    "sourceMap": false,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
  },
  "exclude": ["node_modules", "vendor"],
  "include": [
    "src/Views/react/**/*.ts",
    "src/Views/react/**/*.tsx"
  ]
}
